{
    // "context": "..",
    // "dockerFile": "../Dockerfile",
    "image": "twdu-dev-container",
    "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter",
        "hashicorp.terraform"
    ],
    "settings": {
        "jupyter.alwaysTrustNotebooks": true,
        "jupyter.ignoreVscodeTheme": true,
        "editor.acceptSuggestionOnEnter": "off",
        "files.autoSave": "afterDelay",
        "python.analysis.useImportHeuristic": true
    }, 
    "postCreateCommand": "pip install -e /workspaces/twdu-europe/twdu-tdd/ /workspaces/twdu-europe/data-ingestion/ /workspaces/twdu-europe/data-transformation/"
}